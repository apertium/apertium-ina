  <alphabet>AaBbCcDdEeFfGgHhIiJjKkLlMmNnOoPpQqRrSsTtUuVvWwXxYyZz</alphabet>

  <sdefs>
    <sdef n="n"     c="Noun"/>
    <sdef n="m"     c="Masculine"/>
    <sdef n="f"     c="Feminine"/>
    <sdef n="mf"     c="Masculine/feminine"/>
    <sdef n="sg"    c="Singular"/>
    <sdef n="pl"    c="Plural"/>
    <sdef n="vblex" c="Verb"/>
    <sdef n="p1"    c="1st person"/>
    <sdef n="p2"    c="2nd person"/>
    <sdef n="p3"    c="3rd person"/>
    <sdef n="pri"   c="Present indicative"/>
    <sdef n="prn"   c="Pronoun"/>
    <sdef n="nom"   c="Nominative"/>
    <sdef n="ind"   c="Indefinite"/>/>
    <sdef n="def"   c="Definite"/>
    <sdef n="det"   c="Determiner"/>
    <sdef n="sp"    c="Singular/plural"/>
    <sdef n="preadv"  c="Pre-adverb"/>
    <sdef n="predet"  c="Pre-determiner"/>
    <sdef n="cnjcoo"  c="Coordinating conjunction"/>
    <sdef n="cnjadv"  c="Conjunctive adverb"/>
    <sdef n="pr"    c="Pronoun"/>
    <sdef n="dem"    c="Demonstrative"/>
    <sdef n="adv"    c="Adverb"/>
  </sdefs>

  <pardefs>
    <!-- Define paradigms within <pardef> here. -->
    <pardef n="can__n">
      <e><p><l></l><r><s n="n"/><s n="sg"/></r></p></e>
      <e><p><l>es</l><r><s n="n"/><s n="pl"/></r></p></e>
    </pardef>
    <pardef n="homine__n">
      <e><p><l></l><r><s n="n"/><s n="sg"/></r></p></e>
      <e><p><l>s</l><r><s n="n"/><s n="pl"/></r></p></e>
    </pardef>
    <pardef n="le__det">
      <e><p><l></l><r><s n="det"/><s n="def"/><s n="sp"/></r></p></e>
      <e r="RL"><p><l></l><r><s n="det"/><s n="def"/><s n="sg"/></r></p></e>
    <e r="RL"><p><l>s</l><r><s n="det"/><s n="def"/><s n="pl"/></r></p></e>
    </pardef>
    <pardef n="in__pr">
      <e><p><l></l><r><s n="pr"/></r></p></e>
    </pardef>
    <pardef n="un__det">
      <e><p><l></l><r><s n="det"/><s n="ind"/><s n="sp"/></r></p></e>
    </pardef>
    <pardef n="iste__det">
      <e><p><l></l><r><s n="det"/><s n="dem"/><s n="sp"/></r></p></e>
    </pardef>
    <pardef n="plus__preadv">
      <e><p><l></l><r><s n="preadv"/></r></p></e>
    </pardef>
    <pardef n="tote__predet">
        <e><p><l></l><r><s n="predet"/><s n="sp"/></r></p></e>
    </pardef>
    <pardef n="e__cnjcoo">
      <e>
        <p>
          <l></l>
          <r><s n="cnjcoo"/></r>
        </p>
      </e>
    </pardef>
    <pardef n="si__cnjadv">
      <e>
        <p>
          <l></l>
          <r><s n="cnjadv"/></r>
        </p>
      </e>
    </pardef>
    <pardef n="ora__adv">
      <e>
        <p>
          <l></l>
          <r><s n="adv"/></r>
        </p>
      </e>
    </pardef>
    <pardef n="parl/ar__vblex">
      <e><p><l>a</l><r>ar<s n="vblex"/><s n="pri"/></r></p></e>
    </pardef>
    <pardef n="bib/er__vblex">
      <e><p><l>e</l><r>er<s n="vblex"/><s n="pri"/></r></p></e>
    </pardef>
      
    <pardef n="io__prn">
            <e i=""><p><l/><r><s n="prn"/><s n="nom"/><s n="p1"/><s n="mf"/><s n="sg"/></r></p></e>
    </pardef>
    <pardef n="tu__prn">
