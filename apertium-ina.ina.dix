    </pardef>
    <pardef n="illo__prn">
            <e i=""><p><l/><r><s n="prn"/><s n="nom"/><s n="p3"/><s n="mf"/><s n="sg"/></r></p></e>
    </pardef>
    <pardef n="nos__prn">
            <e i=""><p><l/><r><s n="prn"/><s n="nom"/><s n="p1"/><s n="mf"/><s n="pl"/></r></p></e>
    </pardef>
    <pardef n="vos__prn">
            <e i=""><p><l/><r><s n="prn"/><s n="nom"/><s n="p2"/><s n="mf"/><s n="pl"/></r></p></e>
    </pardef>
    <pardef n="illos__prn">
            <e i=""><p><l/><r><s n="prn"/><s n="nom"/><s n="p3"/><s n="mf"/><s n="pl"/></r></p></e>
    </pardef>

  </pardefs>
    
