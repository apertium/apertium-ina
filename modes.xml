<?xml version="1.0" encoding="UTF-8"?>
<modes>

  <mode name="ina-morph" install="yes">
    <pipeline>
      <program name="lt-proc -w">
        <file name="ina.automorf.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="ina-gener" install="yes">
    <pipeline>
      <program name="lt-proc -g">
        <file name="ina.autogen.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="ina-tagger" install="yes">
    <pipeline>
      <program name="lt-proc -w">
        <file name="ina.automorf.bin"/>
      </program>
      <program name="cg-proc -w">
        <file name="ina.rlx.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="ina-disam" install="yes">
    <pipeline>
      <program name="lt-proc -w">
        <file name="ina.automorf.bin"/>
      </program>
      <program name="cg-conv -a -l" />
      <program name="vislcg3 --trace -g">
        <file name="apertium-ina.ina.rlx"/>
      </program>
    </pipeline>
  </mode>

</modes>

